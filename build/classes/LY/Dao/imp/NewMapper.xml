<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="LY.dao.NewDao">
	
	<insert id="add">
	insert into `new`(`id`,`name`,`age`,`sex`)values(#{id},#{name},#{age},#{sex}) 
	</insert>
    <select id="selectAll" resultType="LY.Entity.New">
  	 select * from new
    </select> 
     </mapper>